algoritmo "exercício1"
var
   anoatual,anonasc,idade: inteiro
inicio
      Escreva("Em que ano nós estamos?? ")
      leia(anoatual)
      Escreva("Em que ano eu nasci?? ")
      leia(anonasc)
      idade<- (anoatual-anonasc)
      Escreva("Então eu tenho",  idade, " anos!! Muito obrigado!!")
fimalgoritmo