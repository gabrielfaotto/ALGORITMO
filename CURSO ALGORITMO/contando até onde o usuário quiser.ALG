algoritmo "semnome"
var
   cont,limi,salto: inteiro
inicio
      escreva("Até quanto você gostaria de avançar? ")
      leia(limi)
      escreva("Qual será o valor do salto? ")
      leia(salto)
      cont <- 0
      enquanto (cont <= limi) faca
               escreval(cont)
               cont <- cont + salto
      fimenquanto
fimalgoritmo